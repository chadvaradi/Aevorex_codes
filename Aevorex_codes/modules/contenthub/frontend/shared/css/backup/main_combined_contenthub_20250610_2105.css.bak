}


.ch-grid-span-1 { grid-column: span 1; }
.ch-grid-span-2 { grid-column: span 2; }
.ch-grid-span-3 { grid-column: span 3; }
.ch-grid-span-4 { grid-column: span 4; }
.ch-grid-span-5 { grid-column: span 5; }
.ch-grid-span-6 { grid-column: span 6; }
.ch-grid-span-7 { grid-column: span 7; }
.ch-grid-span-8 { grid-column: span 8; }
.ch-grid-span-9 { grid-column: span 9; }
.ch-grid-span-10 { grid-column: span 10; }
.ch-grid-span-11 { grid-column: span 11; }
.ch-grid-span-12 { grid-column: span 12; }


.ch-grid-row-span-1 { grid-row: span 1; }
.ch-grid-row-span-2 { grid-row: span 2; }
.ch-grid-row-span-3 { grid-row: span 3; }



.ch-widget {
    background-color: var(--ch-bg-card);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-widget-border-radius);
    box-shadow: var(--ch-shadow-sm);
    overflow: hidden;
    transition: var(--ch-transition-shadow);
    min-height: var(--ch-widget-min-height);
    display: flex;
    flex-direction: column;
}

.ch-widget:hover {
    box-shadow: var(--ch-shadow-md);
}

.ch-widget--interactive {
    cursor: pointer;
}

.ch-widget--highlighted {
    border-color: var(--ch-brand-primary);
    box-shadow: 0 0 0 1px var(--ch-brand-primary), var(--ch-shadow-md);
}


.ch-widget-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: var(--ch-space-4);
    border-bottom: 1px solid var(--ch-border-primary);
    background-color: var(--ch-bg-secondary);
}

.ch-widget-title {
    font-size: 1rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0;
}

.ch-widget-subtitle {
    font-size: 0.75rem;
    color: var(--ch-text-tertiary);
    margin: var(--ch-space-1) 0 0 0;
}

.ch-widget-actions {
    display: flex;
    align-items: center;
    gap: var(--ch-space-2);
}


.ch-widget-body {
    flex: 1;
