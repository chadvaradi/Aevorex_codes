    border: 1px solid var(--ch-border-secondary);
    border-radius: var(--ch-radius-sm);
    padding: var(--ch-space-3);
    min-height: 120px;
    color: var(--ch-text-primary);
    font-size: var(--ch-text-xs);
    line-height: 1.4;
    white-space: pre-wrap;
    overflow-wrap: break-word;
}



.btn {
    
    display: inline-flex;
    align-items: center;
    justify-content: center;
    padding: var(--ch-space-2-5) var(--ch-space-4);
    background: var(--ch-bg-secondary);
    color: var(--ch-text-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-sm);
    font-family: var(--ch-font-family-sans);
    font-weight: var(--ch-font-weight-medium);
    font-size: var(--ch-text-sm);
    cursor: pointer;
    transition: var(--ch-transition-all);
    text-decoration: none;
    user-select: none;
}

.btn:hover {
    background: var(--ch-bg-tertiary);
    border-color: var(--ch-border-secondary);
    transform: translateY(-1px);
    box-shadow: var(--ch-shadow-sm);
}

.btn:active {
    transform: translateY(0);
    box-shadow: var(--ch-shadow-xs);
}

.btn:focus {
    outline: 2px solid var(--ch-brand-primary);
    outline-offset: 2px;
}



.test-section {
    background: var(--ch-bg-primary);
    border: 2px dashed var(--ch-border-secondary);
    border-radius: var(--ch-radius-lg);
    padding: var(--ch-space-6);
    margin: var(--ch-space-8) 0;
}

.test-section h2 {
    margin: 0 0 var(--ch-space-4) 0;
    color: var(--ch-brand-primary);
    font-family: var(--ch-font-family-sans);
    font-weight: var(--ch-font-weight-bold);
    font-size: var(--ch-text-xl);
}

.test-section p {
    margin: 0 0 var(--ch-space-4) 0;
    color: var(--ch-text-secondary);
    font-family: var(--ch-font-family-sans);
    font-size: var(--ch-text-base);
    line-height: 1.6;
}



[data-theme="dark"] .test-navigation {
    background: var(--ch-surface-secondary);
    border-color: var(--ch-border-primary);
}

[data-theme="dark"] .test-navigation .nav-item {
    background: var(--ch-surface-primary);
    color: var(--ch-text-primary);
}

[data-theme="dark"] .debug-console {
    background: var(--ch-surface-primary);
    border-color: var(--ch-border-primary);
}

[data-theme="dark"] .test-section {
    background: var(--ch-surface-primary);
    border-color: var(--ch-border-secondary);
}



@media (max-width: 768px) {
    .module-cards {
        grid-template-columns: 1fr;
        gap: var(--ch-space-3);
        padding: var(--ch-space-3);
    }

    .test-navigation {
        flex-direction: column;
    }

    .test-section {
        padding: var(--ch-space-4);
        margin: var(--ch-space-4) 0;
    }
}






.debug-only,
.test-section.debug-only {
    display: none !important;
    visibility: hidden !important;
    opacity: 0 !important;
    height: 0 !important;
    overflow: hidden !important;
}


body.debug-mode .debug-only,
body.debug-mode .test-section.debug-only {
    display: block !important;
    visibility: visible !important;
    opacity: 1 !important;
    height: auto !important;
    overflow: visible !important;
} 




.contenthub {
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
    background: var(--ch-color-background);
    color: var(--ch-color-text);
    min-height: 100vh;
}

.contenthub[data-theme="dark"] {
    background: var(--ch-dark-background);
    color: var(--ch-dark-text);
}

.contenthub[data-theme="light"] {
    background: var(--ch-light-background);
    color: var(--ch-light-text);
}



.contenthub {
    background: #0a0a0a;
    color: #ffffff;
}

.header {
    background: rgba(255, 255, 255, 0.05);
    backdrop-filter: blur(10px);
    border-bottom: 1px solid rgba(255, 255, 255, 0.1);
}

.btn {
    padding: 0.5rem 1rem;
    border-radius: 0.375rem;
    background: #8b5cf6;
    color: white;
    border: none;
    cursor: pointer;
    transition: all 0.2s ease;
}

.btn:hover {
    background: #7c3aed;
}

.gradient-text {
    background: linear-gradient(135deg, #8b5cf6, #06b6d4);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
}