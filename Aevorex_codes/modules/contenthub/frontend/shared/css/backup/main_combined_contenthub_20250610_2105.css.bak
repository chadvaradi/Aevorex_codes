    --ch-layout-content-max-width: 1200px;
    --ch-layout-content-padding: 1.5rem;

    
    --ch-layout-gap-xs: 0.5rem;
    --ch-layout-gap-sm: 0.75rem;
    --ch-layout-gap-md: 1rem;
    --ch-layout-gap-lg: 1.5rem;
    --ch-layout-gap-xl: 2rem;
    --ch-layout-gap-2xl: 3rem;

    
    --ch-layout-z-base: 1;
    --ch-layout-z-content: 10;
    --ch-layout-z-sidebar: 20;
    --ch-layout-z-header: 30;
    --ch-layout-z-overlay: 40;
    --ch-layout-z-modal: 50;
}



.ch-app {
    display: flex;
    flex-direction: column;
    min-height: 100vh;
    background: var(--ch-bg-primary);
    color: var(--ch-text-primary);
    overflow-x: hidden;
}

.ch-app--with-sidebar {
    display: grid;
    grid-template-areas: 
        "header header"
        "sidebar main";
    grid-template-columns: var(--ch-layout-sidebar-width) 1fr;
    grid-template-rows: var(--ch-layout-header-height) 1fr;
}

.ch-app--sidebar-collapsed {
    grid-template-columns: var(--ch-layout-sidebar-collapsed) 1fr;
}

.ch-app--no-sidebar {
    display: grid;
    grid-template-areas: 
        "header"
        "main";
    grid-template-columns: 1fr;
    grid-template-rows: var(--ch-layout-header-height) 1fr;
}



.ch-layout-header {
    grid-area: header;
    position: sticky;
    top: 0;
    z-index: var(--ch-layout-z-header);

    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 var(--ch-layout-content-padding);

    background: var(--ch-bg-primary);
    border-bottom: 1px solid var(--ch-border-primary);
    backdrop-filter: blur(8px);
    -webkit-backdrop-filter: blur(8px);
}

.ch-layout-header__left {
    display: flex;
    align-items: center;
    gap: var(--ch-layout-gap-lg);
}

.ch-layout-header__center {
    display: flex;
    align-items: center;
    justify-content: center;
    flex: 1;
    max-width: 600px;
    margin: 0 auto;
}

.ch-layout-header__right {
    display: flex;
    align-items: center;
    gap: var(--ch-layout-gap-md);
}



.ch-layout-sidebar {
    grid-area: sidebar;
    position: sticky;
    top: var(--ch-layout-header-height);
    height: calc(100vh - var(--ch-layout-header-height));
    z-index: var(--ch-layout-z-sidebar);

    display: flex;
    flex-direction: column;

    background: var(--ch-bg-primary);
    border-right: 1px solid var(--ch-border-primary);
    overflow-y: auto;
    overflow-x: hidden;

    transition: width 0.3s ease;
}

.ch-layout-sidebar--collapsed {
    width: var(--ch-layout-sidebar-collapsed);
}

.ch-layout-sidebar__content {
    padding: var(--ch-layout-gap-lg) var(--ch-layout-gap-md);
    flex: 1;
}

.ch-layout-sidebar__footer {
    padding: var(--ch-layout-gap-md);
    border-top: 1px solid var(--ch-border-primary);
    margin-top: auto;
}



.ch-layout-main {
    grid-area: main;
    position: relative;
    z-index: var(--ch-layout-z-content);

    display: flex;
    flex-direction: column;
    min-height: calc(100vh - var(--ch-layout-header-height));

    background: var(--ch-bg-primary);
    overflow-x: auto;
}


.ch-main {
    display: flex;
    flex-direction: column;
    min-height: calc(100vh - var(--ch-layout-header-height));
    background: var(--ch-bg-primary);
    flex: 1;
    width: 100%;
}

.ch-layout-content {
    flex: 1;
    padding: var(--ch-layout-content-padding);
    max-width: var(--ch-layout-content-max-width);
    margin: 0 auto;
    width: 100%;
}

.ch-layout-content--full-width {
    max-width: none;
}

.ch-layout-content--narrow {
    max-width: 800px;
}

.ch-layout-content--wide {
    max-width: 1400px;
}



.ch-section {
    margin-bottom: var(--ch-layout-gap-2xl);
}

.ch-section:last-child {
    margin-bottom: 0;
}

.ch-section__header {
    margin-bottom: var(--ch-layout-gap-lg);
    padding-bottom: var(--ch-layout-gap-md);
    border-bottom: 1px solid var(--ch-border-primary);
}

.ch-section__title {
    font-size: var(--ch-text-2xl);
    font-weight: var(--ch-font-weight-bold);
    color: var(--ch-text-primary);
    margin: 0;
}

.ch-section__subtitle {
    font-size: var(--ch-text-sm);
    color: var(--ch-text-secondary);
    margin: 0.5rem 0 0 0;
}

.ch-section__content {
    
}



.ch-grid {
    display: grid;
    gap: var(--ch-layout-gap-lg);
}

.ch-grid--cols-1 {
    grid-template-columns: 1fr;
}

.ch-grid--cols-2 {
    grid-template-columns: repeat(2, 1fr);
}

.ch-grid--cols-3 {
    grid-template-columns: repeat(3, 1fr);
}

.ch-grid--cols-4 {
    grid-template-columns: repeat(4, 1fr);
}

.ch-grid--cols-auto {
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
}

.ch-grid--gap-sm {
    gap: var(--ch-layout-gap-sm);
}

.ch-grid--gap-md {
    gap: var(--ch-layout-gap-md);
}

.ch-grid--gap-lg {
    gap: var(--ch-layout-gap-lg);
}

.ch-grid--gap-xl {
    gap: var(--ch-layout-gap-xl);
}


.ch-grid-item--span-2 {
    grid-column: span 2;
}

.ch-grid-item--span-3 {
    grid-column: span 3;
}

.ch-grid-item--span-4 {
    grid-column: span 4;
}

.ch-grid-item--span-full {
    grid-column: 1 / -1;
}



.ch-flex {
    display: flex;
}

.ch-flex--column {
    flex-direction: column;
}

.ch-flex--row {
    flex-direction: row;
}

.ch-flex--wrap {
    flex-wrap: wrap;
}

.ch-flex--center {
    align-items: center;
    justify-content: center;
}

.ch-flex--between {
    justify-content: space-between;
}

.ch-flex--around {
    justify-content: space-around;
}

.ch-flex--evenly {
    justify-content: space-evenly;
}

.ch-flex--start {
    justify-content: flex-start;
}

.ch-flex--end {
    justify-content: flex-end;
}

.ch-flex--items-start {
    align-items: flex-start;
}

.ch-flex--items-center {
    align-items: center;
}

.ch-flex--items-end {
    align-items: flex-end;
}

.ch-flex--items-stretch {
    align-items: stretch;
}


.ch-flex-1 {
    flex: 1;
}

.ch-flex-none {
    flex: none;
}

.ch-flex-auto {
    flex: auto;
}



.ch-gap-xs { gap: var(--ch-layout-gap-xs); }
.ch-gap-sm { gap: var(--ch-layout-gap-sm); }
.ch-gap-md { gap: var(--ch-layout-gap-md); }
.ch-gap-lg { gap: var(--ch-layout-gap-lg); }
.ch-gap-xl { gap: var(--ch-layout-gap-xl); }
.ch-gap-2xl { gap: var(--ch-layout-gap-2xl); }


.ch-m-0 { margin: 0; }
