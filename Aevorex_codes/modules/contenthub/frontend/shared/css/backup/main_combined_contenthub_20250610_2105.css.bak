.ch-font-black { font-weight: var(--ch-font-weight-black); }


.ch-font-sans { font-family: var(--ch-font-family-sans); }
.ch-font-serif { font-family: var(--ch-font-family-serif); }
.ch-font-mono { font-family: var(--ch-font-family-mono); }


.ch-leading-none { line-height: var(--ch-line-height-none); }
.ch-leading-tight { line-height: var(--ch-line-height-tight); }
.ch-leading-snug { line-height: var(--ch-line-height-snug); }
.ch-leading-normal { line-height: var(--ch-line-height-normal); }
.ch-leading-relaxed { line-height: var(--ch-line-height-relaxed); }
.ch-leading-loose { line-height: var(--ch-line-height-loose); }


.ch-tracking-tighter { letter-spacing: var(--ch-letter-spacing-tighter); }
.ch-tracking-tight { letter-spacing: var(--ch-letter-spacing-tight); }
.ch-tracking-normal { letter-spacing: var(--ch-letter-spacing-normal); }
.ch-tracking-wide { letter-spacing: var(--ch-letter-spacing-wide); }
.ch-tracking-wider { letter-spacing: var(--ch-letter-spacing-wider); }
.ch-tracking-widest { letter-spacing: var(--ch-letter-spacing-widest); }


.ch-text-left { text-align: left; }
.ch-text-center { text-align: center; }
.ch-text-right { text-align: right; }
.ch-text-justify { text-align: justify; }


.ch-uppercase { text-transform: uppercase; }
.ch-lowercase { text-transform: lowercase; }
.ch-capitalize { text-transform: capitalize; }
.ch-normal-case { text-transform: none; }


.ch-underline { text-decoration: underline; }
.ch-line-through { text-decoration: line-through; }
.ch-no-underline { text-decoration: none; }


.ch-truncate {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ch-text-ellipsis {
    text-overflow: ellipsis;
}

.ch-text-clip {
    text-overflow: clip;
}



code,
kbd,
samp,
pre {
    font-family: var(--ch-font-family-mono);
    font-feature-settings: "kern" 0, "liga" 0, "calt" 0;
}

code {
    font-size: 0.9em;
    color: var(--ch-text-primary);
    background-color: var(--ch-bg-tertiary);
    padding: 0.125rem 0.25rem;
    border-radius: var(--ch-radius-sm);
    border: 1px solid var(--ch-border-primary);
}

pre {
    font-size: var(--ch-font-size-sm);
    line-height: var(--ch-line-height-relaxed);
    color: var(--ch-text-primary);
    background-color: var(--ch-bg-tertiary);
    padding: var(--ch-space-4);
    border-radius: var(--ch-radius-md);
    border: 1px solid var(--ch-border-primary);
    overflow-x: auto;
    margin: 0 0 var(--ch-space-4) 0;
}

pre code {
    background: none;
    border: none;
    padding: 0;
    font-size: inherit;
}

kbd {
    font-size: 0.9em;
    color: var(--ch-text-inverse);
    background-color: var(--ch-text-primary);
    padding: 0.125rem 0.375rem;
    border-radius: var(--ch-radius-sm);
    border: 1px solid var(--ch-border-secondary);
    box-shadow: inset 0 -1px 0 rgba(0, 0, 0, 0.25);
}



blockquote {
    font-size: var(--ch-font-size-lg);
    font-style: italic;
    line-height: var(--ch-line-height-relaxed);
    color: var(--ch-text-secondary);
    border-left: 4px solid var(--ch-border-secondary);
    padding-left: var(--ch-space-4);
    margin: 0 0 var(--ch-space-4) 0;
}

blockquote p {
    margin: 0;
}

blockquote cite {
    font-size: var(--ch-font-size-sm);
    font-style: normal;
    font-weight: var(--ch-font-weight-medium);
    color: var(--ch-text-tertiary);
    display: block;
    margin-top: var(--ch-space-2);
}

blockquote cite::before {
    content: "— ";
}



.ch-list {
    margin: 0 0 var(--ch-space-4) 0;
    padding-left: var(--ch-space-5);
}

.ch-list-ordered {
    list-style-type: decimal;
}

.ch-list-unordered {
    list-style-type: disc;
}

.ch-list li {
    margin-bottom: var(--ch-space-2);
    line-height: var(--ch-line-height-relaxed);
}

.ch-list li:last-child {
    margin-bottom: 0;
}

.ch-list-none {
    list-style: none;
    padding-left: 0;
}



@media (max-width: 768px) {
    h1, .ch-text-h1 {
        font-size: var(--ch-font-size-3xl);
        line-height: var(--ch-line-height-snug);
    }

    h2, .ch-text-h2 {
        font-size: var(--ch-font-size-2xl);
        line-height: var(--ch-line-height-snug);
    }

    h3, .ch-text-h3 {
        font-size: var(--ch-font-size-xl);
    }

    h4, .ch-text-h4 {
        font-size: var(--ch-font-size-lg);
    }

    .ch-text-lead {
        font-size: var(--ch-font-size-base);
    }
}




@media (prefers-contrast: high) {
    code,
    pre {
        border-width: 2px;
    }

    blockquote {
        border-left-width: 6px;
    }
}


@media (prefers-reduced-motion: reduce) {
    * {
        text-decoration-skip-ink: none;
    }
}


a:focus-visible,
button:focus-visible {
    outline: 2px solid var(--ch-brand-primary);
    outline-offset: 2px;
    border-radius: var(--ch-radius-sm);
}



 





.ch-btn {
    
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: var(--ch-space-2);

    
    font-family: var(--ch-font-family-sans);
    font-weight: var(--ch-font-weight-medium);
    text-align: center;
    text-decoration: none;
    white-space: nowrap;

    
    padding: var(--ch-space-2-5) var(--ch-space-5);
    min-height: 2.5rem;
    font-size: var(--ch-text-sm);
    line-height: 1.25;

    
    border: 1px solid transparent;
    border-radius: var(--ch-radius-md);
    background: transparent;
    cursor: pointer;

    
    transition: all 0.15s ease;
    transform: translateY(0);

    
    user-select: none;
    -webkit-user-select: none;
    -webkit-tap-highlight-color: transparent;
}

.ch-btn:focus {
    outline: none;
}

.ch-btn:focus-visible {
    outline: 2px solid var(--ch-brand-primary);
    outline-offset: 2px;
}

.ch-btn:active {
    transform: translateY(1px);
}

.ch-btn:disabled,
.ch-btn[aria-disabled="true"] {
    cursor: not-allowed;
    opacity: 0.5;
    transform: none;
}




.ch-btn--primary {
    background: linear-gradient(135deg, var(--ch-brand-primary), var(--ch-brand-600));
    color: var(--ch-white);
    border-color: var(--ch-brand-primary);
    box-shadow: var(--ch-shadow-sm);
}

.ch-btn--primary:hover:not(:disabled) {
    background: linear-gradient(135deg, var(--ch-brand-600), var(--ch-brand-700));
    box-shadow: var(--ch-shadow-md);
    transform: translateY(-1px);
}

.ch-btn--primary:active:not(:disabled) {
    background: var(--ch-brand-700);
    box-shadow: var(--ch-shadow-sm);
    transform: translateY(0);
}


.ch-btn--secondary {
    background: var(--ch-bg-secondary);
    color: var(--ch-text-primary);
    border-color: var(--ch-border-primary);
    box-shadow: var(--ch-shadow-xs);
}

.ch-btn--secondary:hover:not(:disabled) {
    background: var(--ch-bg-tertiary);
    border-color: var(--ch-border-secondary);
    box-shadow: var(--ch-shadow-sm);
    transform: translateY(-1px);
}

.ch-btn--secondary:active:not(:disabled) {
    background: var(--ch-bg-quaternary);
    box-shadow: var(--ch-shadow-xs);
    transform: translateY(0);
}


.ch-btn--ghost {
    background: transparent;
    color: var(--ch-text-secondary);
    border-color: transparent;
}

.ch-btn--ghost:hover:not(:disabled) {
    background: var(--ch-bg-secondary);
    color: var(--ch-text-primary);
    transform: translateY(-1px);
}

.ch-btn--ghost:active:not(:disabled) {
    background: var(--ch-bg-tertiary);
    transform: translateY(0);
}


.ch-btn--outline {
    background: transparent;
    color: var(--ch-brand-primary);
    border-color: var(--ch-brand-primary);
}

.ch-btn--outline:hover:not(:disabled) {
    background: var(--ch-brand-50);
    color: var(--ch-brand-600);
    border-color: var(--ch-brand-600);
    transform: translateY(-1px);
}

.ch-btn--outline:active:not(:disabled) {
    background: var(--ch-brand-100);
    color: var(--ch-brand-700);
    border-color: var(--ch-brand-700);
    transform: translateY(0);
}


.ch-btn--danger {
    background: linear-gradient(135deg, var(--ch-error-500), var(--ch-error-600));
    color: var(--ch-white);
    border-color: var(--ch-error-500);
    box-shadow: var(--ch-shadow-sm);
}

.ch-btn--danger:hover:not(:disabled) {
    background: linear-gradient(135deg, var(--ch-error-600), var(--ch-error-700));
    box-shadow: var(--ch-shadow-md);
    transform: translateY(-1px);
}

.ch-btn--danger:active:not(:disabled) {
    background: var(--ch-error-700);
    box-shadow: var(--ch-shadow-sm);
    transform: translateY(0);
}


.ch-btn--success {
    background: linear-gradient(135deg, var(--ch-success-500), var(--ch-success-600));
    color: var(--ch-white);
    border-color: var(--ch-success-500);
    box-shadow: var(--ch-shadow-sm);
}

.ch-btn--success:hover:not(:disabled) {
    background: linear-gradient(135deg, var(--ch-success-600), var(--ch-success-700));
    box-shadow: var(--ch-shadow-md);
    transform: translateY(-1px);
}

.ch-btn--success:active:not(:disabled) {
    background: var(--ch-success-700);
    box-shadow: var(--ch-shadow-sm);
    transform: translateY(0);
}



.ch-btn--xs {
    padding: 0.375rem 0.75rem;
    min-height: 1.75rem;
    font-size: var(--ch-text-xs);
    border-radius: var(--ch-radius-sm);
}

.ch-btn--sm {
    padding: 0.5rem 1rem;
    min-height: 2rem;
    font-size: var(--ch-text-sm);
    border-radius: var(--ch-radius-sm);
}

.ch-btn--md {
    
}

.ch-btn--lg {
    padding: 0.75rem 1.5rem;
    min-height: 3rem;
    font-size: var(--ch-text-base);
    border-radius: var(--ch-radius-lg);
}

.ch-btn--xl {
    padding: 1rem 2rem;
    min-height: 3.5rem;
    font-size: var(--ch-text-lg);
    border-radius: var(--ch-radius-lg);
}




.ch-btn--full {
    width: 100%;
}


.ch-btn--icon-only {
    padding: 0;
    width: 2.5rem;
    height: 2.5rem;
    aspect-ratio: 1;
}

.ch-btn--icon-only.ch-btn--xs {
    width: 1.75rem;
    height: 1.75rem;
}

.ch-btn--icon-only.ch-btn--sm {
    width: 2rem;
    height: 2rem;
}

.ch-btn--icon-only.ch-btn--lg {
    width: 3rem;
    height: 3rem;
}

.ch-btn--icon-only.ch-btn--xl {
    width: 3.5rem;
    height: 3.5rem;
}


.ch-btn--loading {
    position: relative;
    color: transparent;
    pointer-events: none;
}

.ch-btn--loading::after {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 1rem;
    height: 1rem;
    margin: -0.5rem 0 0 -0.5rem;
    border: 2px solid transparent;
    border-top-color: currentColor;
    border-radius: 50%;
    animation: ch-btn-spin 0.8s linear infinite;
}

.ch-btn--loading.ch-btn--xs::after {
    width: 0.75rem;
    height: 0.75rem;
    margin: -0.375rem 0 0 -0.375rem;
}

.ch-btn--loading.ch-btn--sm::after {
    width: 0.875rem;
    height: 0.875rem;
    margin: -0.4375rem 0 0 -0.4375rem;
}

.ch-btn--loading.ch-btn--lg::after {
    width: 1.25rem;
    height: 1.25rem;
    margin: -0.625rem 0 0 -0.625rem;
}

.ch-btn--loading.ch-btn--xl::after {
    width: 1.5rem;
    height: 1.5rem;
    margin: -0.75rem 0 0 -0.75rem;
}

@keyframes ch-btn-spin {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
}



.ch-btn-group {
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
}

.ch-btn-group--attached {
    gap: 0;
}

.ch-btn-group--attached .ch-btn {
    border-radius: 0;
    border-right-width: 0;
}

.ch-btn-group--attached .ch-btn:first-child {
    border-radius: var(--ch-radius-md) 0 0 var(--ch-radius-md);
}

.ch-btn-group--attached .ch-btn:last-child {
    border-radius: 0 var(--ch-radius-md) var(--ch-radius-md) 0;
    border-right-width: 1px;
}

.ch-btn-group--attached .ch-btn:only-child {
    border-radius: var(--ch-radius-md);
    border-right-width: 1px;
}



.ch-btn__icon {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    width: 1rem;
    height: 1rem;
    flex-shrink: 0;
}

.ch-btn--xs .ch-btn__icon {
    width: 0.75rem;
    height: 0.75rem;
}

.ch-btn--sm .ch-btn__icon {
    width: 0.875rem;
    height: 0.875rem;
}

.ch-btn--lg .ch-btn__icon {
    width: 1.25rem;
    height: 1.25rem;
}

.ch-btn--xl .ch-btn__icon {
    width: 1.5rem;
    height: 1.5rem;
}

.ch-btn__icon svg {
    width: 100%;
    height: 100%;
    fill: currentColor;
}



@media (max-width: 640px) {
    .ch-btn--responsive-full {
        width: 100%;
    }

    .ch-btn-group--responsive-stack {
        flex-direction: column;
        width: 100%;
    }

    .ch-btn-group--responsive-stack .ch-btn {
        width: 100%;
    }
}



@media (prefers-reduced-motion: reduce) {
    .ch-btn {
        transition: none;
    }

    .ch-btn:hover,
    .ch-btn:active {
        transform: none;
    }

    .ch-btn--loading::after {
        animation: none;
    }
}

@media (prefers-contrast: high) {
    .ch-btn {
        border-width: 2px;
    }
}



 







:root {
    
    --ch-card-padding-sm: var(--ch-space-3);
    --ch-card-padding-md: var(--ch-space-4);
    --ch-card-padding-lg: var(--ch-space-6);

    
    --ch-card-bg: var(--ch-bg-card);
    --ch-card-border: var(--ch-border-primary);
    --ch-card-shadow: var(--ch-shadow-sm);

    
    --ch-card-header-bg: var(--ch-bg-secondary);
    --ch-card-header-border: var(--ch-border-primary);
}



.ch-card {
    display: flex;
    flex-direction: column;
    background-color: var(--ch-card-bg);
    border: 1px solid var(--ch-card-border);
    border-radius: var(--ch-radius-lg);
    box-shadow: var(--ch-card-shadow);
    transition: var(--ch-transition-shadow);
    overflow: hidden;
    position: relative;
}


.ch-card--sm {
    border-radius: var(--ch-radius-md);
}

.ch-card--lg {
    border-radius: var(--ch-radius-xl);
}


.ch-card--flat {
    box-shadow: none;
    border: 1px solid var(--ch-border-primary);
}

.ch-card--elevated {
    box-shadow: var(--ch-shadow-md);
}

.ch-card--floating {
    box-shadow: var(--ch-shadow-lg);
}


.ch-card--interactive {
    cursor: pointer;
    transition: var(--ch-transition-shadow), var(--ch-transition-transform);

    &:hover {
        box-shadow: var(--ch-shadow-md);
        transform: translateY(-1px);
    }

    &:active {
        transform: translateY(0);
        box-shadow: var(--ch-shadow-sm);
    }
}



.ch-card-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: var(--ch-card-padding-md);
    background-color: var(--ch-card-header-bg);
    border-bottom: 1px solid var(--ch-card-header-border);
    min-height: 3.5rem;
}

.ch-card-header--sm {
    padding: var(--ch-card-padding-sm);
    min-height: 2.5rem;
}

.ch-card-header--lg {
    padding: var(--ch-card-padding-lg);
    min-height: 4rem;
}

.ch-card-header--borderless {
    border-bottom: none;
    background-color: transparent;
}


.ch-card-title {
    font-size: 1rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0;
    line-height: 1.4;
    flex: 1;
}

.ch-card-title--sm {
    font-size: 0.875rem;
}

.ch-card-title--lg {
    font-size: 1.125rem;
}


.ch-card-subtitle {
    font-size: 0.75rem;
    font-weight: var(--ch-font-weight-normal);
    color: var(--ch-text-secondary);
    margin: var(--ch-space-1) 0 0 0;
    line-height: 1.4;
}


.ch-card-actions {
    display: flex;
    align-items: center;
    gap: var(--ch-space-2);
    margin-left: var(--ch-space-3);
}



.ch-card-body {
    flex: 1;
    padding: var(--ch-card-padding-md);
    color: var(--ch-text-primary);
    line-height: 1.6;
}

.ch-card-body--sm {
    padding: var(--ch-card-padding-sm);
}

.ch-card-body--lg {
    padding: var(--ch-card-padding-lg);
}

.ch-card-body--borderless {
    padding-top: 0;
}


.ch-card-text {
    font-size: 0.875rem;
    color: var(--ch-text-secondary);
    margin: 0 0 var(--ch-space-3) 0;
    line-height: 1.6;
}

.ch-card-text:last-child {
    margin-bottom: 0;
}



.ch-card-footer {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: var(--ch-card-padding-md);
    background-color: var(--ch-bg-secondary);
    border-top: 1px solid var(--ch-border-primary);
    margin-top: auto;
}

.ch-card-footer--sm {
    padding: var(--ch-card-padding-sm);
}

.ch-card-footer--lg {
    padding: var(--ch-card-padding-lg);
}

.ch-card-footer--borderless {
    border-top: none;
    background-color: transparent;
}

.ch-card-footer--center {
    justify-content: center;
}

.ch-card-footer--end {
    justify-content: flex-end;
}




.ch-card--product {
    .ch-card-header {
        padding-bottom: 0;
        border-bottom: none;
        background-color: transparent;
    }

    .ch-card-body {
        padding-top: var(--ch-space-2);
    }
}


.ch-card--profile {
    text-align: center;

    .ch-card-body {
        padding-top: var(--ch-space-6);
    }
}


.ch-card--stats {
    .ch-card-body {
        display: flex;
        align-items: center;
        justify-content: space-between;
        padding: var(--ch-space-6) var(--ch-card-padding-md);
    }
}


.ch-card--feature {
    border: 2px solid var(--ch-border-primary);

    &:hover {
        border-color: var(--ch-brand-primary);
    }
}



.ch-card-media {
    display: block;
    width: 100%;
    height: auto;
    object-fit: cover;
}

.ch-card-media--aspect-video {
    aspect-ratio: 16 / 9;
}

.ch-card-media--aspect-square {
    aspect-ratio: 1 / 1;
}

.ch-card-media--rounded {
    border-radius: var(--ch-radius-lg) var(--ch-radius-lg) 0 0;
}



.ch-card-avatar {
    width: 3rem;
    height: 3rem;
    border-radius: 50%;
    object-fit: cover;
    border: 2px solid var(--ch-border-primary);
    margin: 0 auto var(--ch-space-3) auto;
}

.ch-card-avatar--sm {
    width: 2rem;
    height: 2rem;
}

.ch-card-avatar--lg {
    width: 4rem;
    height: 4rem;
}

.ch-card-avatar--square {
    border-radius: var(--ch-radius-md);
}



.ch-card-badge {
    position: absolute;
    top: var(--ch-space-3);
    right: var(--ch-space-3);
    display: inline-flex;
    align-items: center;
    justify-content: center;
    min-width: 1.5rem;
    height: 1.5rem;
    padding: 0 var(--ch-space-2);
    font-size: 0.75rem;
    font-weight: var(--ch-font-weight-medium);
    color: var(--ch-white);
    background-color: var(--ch-brand-primary);
    border-radius: var(--ch-radius-full);
    z-index: 1;
}

.ch-card-badge--success {
    background-color: var(--ch-success-500);
}

.ch-card-badge--warning {
    background-color: var(--ch-warning-500);
}

.ch-card-badge--error {
    background-color: var(--ch-error-500);
}



.ch-card--loading {
    pointer-events: none;
    position: relative;

    &::after {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: linear-gradient(
            90deg,
            transparent,
            rgba(255, 255, 255, 0.2),
            transparent
        );
        transform: translateX(-100%);
        animation: shimmer 1.5s infinite;
    }
}

@keyframes shimmer {
    100% {
        transform: translateX(100%);
    }
}



.ch-card-grid {
    display: grid;
    gap: var(--ch-space-6);
    grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
}

.ch-card-grid--2-col {
    grid-template-columns: repeat(2, 1fr);
}

.ch-card-grid--3-col {
    grid-template-columns: repeat(3, 1fr);
}

.ch-card-grid--4-col {
    grid-template-columns: repeat(4, 1fr);
}



@media (max-width: 1024px) {
    .ch-card-grid--4-col {
        grid-template-columns: repeat(3, 1fr);
    }
}

@media (max-width: 768px) {
    .ch-card-grid--3-col,
    .ch-card-grid--4-col {
        grid-template-columns: repeat(2, 1fr);
    }

    .ch-card-header {
        flex-direction: column;
        align-items: flex-start;
        gap: var(--ch-space-2);
    }

    .ch-card-actions {
        margin-left: 0;
        width: 100%;
        justify-content: flex-end;
    }
}

@media (max-width: 480px) {
    .ch-card-grid,
    .ch-card-grid--2-col,
    .ch-card-grid--3-col,
    .ch-card-grid--4-col {
        grid-template-columns: 1fr;
    }

    .ch-card {
        margin: 0 var(--ch-space-2);
    }

    .ch-card-footer {
        flex-direction: column;
        gap: var(--ch-space-2);
        align-items: stretch;
    }
}




.ch-card--interactive:focus {
    outline: 2px solid var(--ch-brand-primary);
    outline-offset: 2px;
}


@media (prefers-contrast: high) {
    .ch-card {
        border-width: 2px;
    }

    .ch-card--interactive:focus {
        outline-width: 3px;
    }
}


@media (prefers-reduced-motion: reduce) {
    .ch-card--interactive {
        transition: none;
    }

    .ch-card--loading::after {
        animation: none;
    }
}



 





:root {
    --ch-nav-height: 4rem;
    --ch-nav-sidebar-width: 16rem;
    --ch-nav-sidebar-collapsed-width: 4rem;
    --ch-nav-z-index: 50;
    --ch-nav-overlay-z-index: 49;
}



.ch-nav-header {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: var(--ch-nav-z-index);

    height: var(--ch-nav-height);
    background: var(--ch-bg-primary);
    border-bottom: 1px solid var(--ch-border-primary);
    backdrop-filter: blur(8px);
    -webkit-backdrop-filter: blur(8px);

    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 1.5rem;

    transition: all 0.3s ease;
}

.ch-nav-header--transparent {
    background: rgba(var(--ch-bg-primary-rgb), 0.8);
    border-bottom-color: rgba(var(--ch-border-primary-rgb), 0.5);
}

.ch-nav-header__brand {
    display: flex;
    align-items: center;
    gap: 0.75rem;
    text-decoration: none;
    color: var(--ch-text-primary);
    font-weight: var(--ch-font-weight-semibold);
    font-size: var(--ch-text-lg);
}

.ch-nav-header__logo {
    width: 2rem;
    height: 2rem;
    object-fit: contain;
}

.ch-nav-header__title {
    font-weight: var(--ch-font-weight-bold);
    background: linear-gradient(135deg, var(--ch-brand-primary), var(--ch-brand-600));
    -webkit-background-clip: text;
    background-clip: text;
    -webkit-text-fill-color: transparent;
}



.ch-nav-menu {
    display: flex;
    align-items: center;
    gap: 2rem;
    list-style: none;
    margin: 0;
    padding: 0;
}

.ch-nav-menu__item {
    position: relative;
}


.ch-nav__item {
    position: relative;
    list-style: none;
}

.ch-nav-menu__link {
    display: flex;
    align-items: center;
    gap: 0.5rem;
    padding: 0.5rem 1rem;
    color: var(--ch-text-secondary);
    text-decoration: none;
    font-weight: var(--ch-font-weight-medium);
    font-size: var(--ch-text-sm);
    border-radius: var(--ch-radius-md);
    transition: all 0.15s ease;
    position: relative;
}

.ch-nav-menu__link:hover {
    color: var(--ch-text-primary);
    background: var(--ch-bg-secondary);
}

.ch-nav-menu__link--active {
    color: var(--ch-brand-primary);
    background: var(--ch-brand-50);
}

.ch-nav-menu__link--active::after {
    content: '';
    position: absolute;
    bottom: -0.75rem;
    left: 50%;
    transform: translateX(-50%);
    width: 0.5rem;
    height: 0.125rem;
    background: var(--ch-brand-primary);
    border-radius: var(--ch-radius-full);
}

.ch-nav-menu__icon {
    width: 1rem;
    height: 1rem;
    fill: currentColor;
}



.ch-nav-toggle {
    display: none;
    background: transparent;
    border: none;
    padding: 0.5rem;
    color: var(--ch-text-primary);
    cursor: pointer;
    border-radius: var(--ch-radius-md);
    transition: background 0.15s ease;
}

.ch-nav-toggle:hover {
    background: var(--ch-bg-secondary);
}

.ch-nav-toggle__icon {
    width: 1.5rem;
    height: 1.5rem;
    fill: currentColor;
}



.ch-nav-sidebar {
    position: fixed;
    top: var(--ch-nav-height);
    left: 0;
    bottom: 0;
    width: var(--ch-nav-sidebar-width);
    z-index: calc(var(--ch-nav-z-index) - 1);

    background: var(--ch-bg-primary);
    border-right: 1px solid var(--ch-border-primary);

    display: flex;
    flex-direction: column;
    overflow-y: auto;

    transition: all 0.3s ease;
    transform: translateX(0);
}

.ch-nav-sidebar--collapsed {
    width: var(--ch-nav-sidebar-collapsed-width);
}

.ch-nav-sidebar--hidden {
    transform: translateX(-100%);
}

.ch-nav-sidebar__content {
    padding: 1.5rem 1rem;
    flex: 1;
}

.ch-nav-sidebar__section {
    margin-bottom: 2rem;
}

.ch-nav-sidebar__section:last-child {
    margin-bottom: 0;
}

.ch-nav-sidebar__title {
    font-size: var(--ch-text-xs);
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-tertiary);
    text-transform: uppercase;
    letter-spacing: var(--ch-tracking-wider);
    margin-bottom: 0.75rem;
    padding: 0 0.75rem;
}

.ch-nav-sidebar__list {
    list-style: none;
    margin: 0;
    padding: 0;
    display: flex;
    flex-direction: column;
    gap: 0.25rem;
}

.ch-nav-sidebar__item {
    position: relative;
}

.ch-nav-sidebar__link {
    display: flex;
    align-items: center;
    gap: 0.75rem;
    padding: 0.75rem;
    color: var(--ch-text-secondary);
    text-decoration: none;
    font-weight: var(--ch-font-weight-medium);
    font-size: var(--ch-text-sm);
    border-radius: var(--ch-radius-md);
    transition: all 0.15s ease;
    position: relative;
}

.ch-nav-sidebar__link:hover {
    color: var(--ch-text-primary);
    background: var(--ch-bg-secondary);
}

.ch-nav-sidebar__link--active {
    color: var(--ch-brand-primary);
    background: var(--ch-brand-50);
    font-weight: var(--ch-font-weight-semibold);
}

.ch-nav-sidebar__link--active::before {
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    width: 0.25rem;
    height: 1.5rem;
    background: var(--ch-brand-primary);
    border-radius: 0 var(--ch-radius-full) var(--ch-radius-full) 0;
}

.ch-nav-sidebar__icon {
    width: 1.25rem;
    height: 1.25rem;
    fill: currentColor;
    flex-shrink: 0;
}

.ch-nav-sidebar__text {
    flex: 1;
    min-width: 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.ch-nav-sidebar--collapsed .ch-nav-sidebar__text,
.ch-nav-sidebar--collapsed .ch-nav-sidebar__title {
    opacity: 0;
    pointer-events: none;
}



.ch-nav-breadcrumb {
    display: flex;
    align-items: center;
    gap: 0.5rem;
    list-style: none;
    margin: 0;
    padding: 0;
    font-size: var(--ch-text-sm);
}

.ch-nav-breadcrumb__item {
    display: flex;
    align-items: center;
    gap: 0.5rem;
}

.ch-nav-breadcrumb__link {
    color: var(--ch-text-secondary);
    text-decoration: none;
    transition: color 0.15s ease;
}

.ch-nav-breadcrumb__link:hover {
    color: var(--ch-brand-primary);
}

.ch-nav-breadcrumb__current {
    color: var(--ch-text-primary);
    font-weight: var(--ch-font-weight-medium);
}

.ch-nav-breadcrumb__separator {
    color: var(--ch-text-tertiary);
    font-size: var(--ch-text-xs);
}



.ch-nav-tabs {
    display: flex;
    align-items: center;
    border-bottom: 1px solid var(--ch-border-primary);
    gap: 0;
    list-style: none;
    margin: 0;
    padding: 0;
}

.ch-nav-tabs__item {
    position: relative;
}

.ch-nav-tabs__link {
    display: flex;
    align-items: center;
    gap: 0.5rem;
    padding: 1rem 1.5rem;
    color: var(--ch-text-secondary);
    text-decoration: none;
    font-weight: var(--ch-font-weight-medium);
    font-size: var(--ch-text-sm);
    border-bottom: 2px solid transparent;
    transition: all 0.15s ease;
    position: relative;
}

.ch-nav-tabs__link:hover {
    color: var(--ch-text-primary);
    background: var(--ch-bg-secondary);
}

.ch-nav-tabs__link--active {
    color: var(--ch-brand-primary);
    border-bottom-color: var(--ch-brand-primary);
    background: transparent;
}

.ch-nav-tabs__icon {
    width: 1rem;
    height: 1rem;
    fill: currentColor;
}



@media (max-width: 768px) {
    .ch-nav-toggle {
        display: block;
    }

    .ch-nav-menu {
        position: fixed;
        top: var(--ch-nav-height);
        left: 0;
        right: 0;
        bottom: 0;
        background: var(--ch-bg-primary);
        border-top: 1px solid var(--ch-border-primary);

        flex-direction: column;
        justify-content: flex-start;
        align-items: stretch;
        gap: 0;
        padding: 1rem;

        transform: translateY(-100vh);
        transition: transform 0.3s ease;
        z-index: var(--ch-nav-overlay-z-index);
    }

    .ch-nav-menu--open {
        transform: translateY(0);
    }

    .ch-nav-menu__item {
        width: 100%;
    }

    .ch-nav-menu__link {
        width: 100%;
        padding: 1rem;
        justify-content: flex-start;
        font-size: var(--ch-text-base);
    }

    .ch-nav-menu__link--active::after {
        display: none;
    }

    .ch-nav-sidebar {
        transform: translateX(-100%);
    }

    .ch-nav-sidebar--mobile-open {
        transform: translateX(0);
    }

    .ch-nav-tabs {
        overflow-x: auto;
        -webkit-overflow-scrolling: touch;
        scrollbar-width: none;
        -ms-overflow-style: none;
    }

    .ch-nav-tabs::-webkit-scrollbar {
        display: none;
    }

    .ch-nav-tabs__link {
        white-space: nowrap;
        padding: 0.75rem 1rem;
    }
}



.ch-nav-overlay {
    position: fixed;
    inset: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: calc(var(--ch-nav-overlay-z-index) - 1);
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
}

.ch-nav-overlay--visible {
    opacity: 1;
    visibility: visible;
}



@media (prefers-reduced-motion: reduce) {
    .ch-nav-header,
    .ch-nav-sidebar,
    .ch-nav-menu,
    .ch-nav-menu__link,
    .ch-nav-sidebar__link,
    .ch-nav-breadcrumb__link,
    .ch-nav-tabs__link,
    .ch-nav-overlay {
        transition: none;
    }
}

@media (prefers-contrast: high) {
    .ch-nav-header {
        border-bottom-width: 2px;
    }

    .ch-nav-sidebar {
        border-right-width: 2px;
    }

    .ch-nav-tabs {
        border-bottom-width: 2px;
    }

    .ch-nav-tabs__link--active {
        border-bottom-width: 3px;
    }
}


.ch-nav-menu__link:focus-visible,
.ch-nav-sidebar__link:focus-visible,
.ch-nav-breadcrumb__link:focus-visible,
.ch-nav-tabs__link:focus-visible {
    outline: 2px solid var(--ch-brand-primary);
    outline-offset: 2px;
}



 





.ch-hero {
    position: relative;
    padding: 4rem 0 6rem 0;
    background: linear-gradient(135deg, 
        var(--ch-bg-primary) 0%, 
        var(--ch-surface-primary) 50%, 
        var(--ch-bg-primary) 100%);
    overflow: hidden;
}

.ch-hero::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: radial-gradient(ellipse at center top, 
        rgba(139, 92, 246, 0.1) 0%, 
        transparent 50%);
    pointer-events: none;
}

.ch-hero .container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 var(--ch-space-4);
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 4rem;
    align-items: center;
    position: relative;
    z-index: 2;
}



.ch-hero__content {
    display: flex;
    flex-direction: column;
    gap: 2rem;
}

.ch-hero__title {
    font-size: clamp(2.5rem, 4vw, 3.5rem);
    font-weight: var(--ch-font-weight-bold);
    line-height: 1.1;
    color: var(--ch-text-primary);
    margin: 0;
    letter-spacing: -0.02em;
}

.ch-hero__subtitle {
    font-size: 1.25rem;
    line-height: 1.6;
    color: var(--ch-text-secondary);
    margin: 0;
    max-width: 500px;
}


.gradient-text {
    background: linear-gradient(135deg, 
        var(--ch-brand-primary) 0%, 
        var(--ch-brand-secondary) 50%, 
        #06b6d4 100%);
    -webkit-background-clip: text;
    background-clip: text;
    -webkit-text-fill-color: transparent;
    background-size: 200% 200%;
    animation: gradientShift 3s ease-in-out infinite alternate;
}

@keyframes gradientShift {
    0% { background-position: 0% 50%; }
    100% { background-position: 100% 50%; }
}



.ch-hero__features {
    display: flex;
    flex-wrap: wrap;
    gap: 1rem;
    margin-top: 1rem;
}

.hero-feature {
    display: flex;
    align-items: center;
    gap: 0.75rem;
    padding: 0.75rem 1rem;
    background: rgba(139, 92, 246, 0.1);
    border: 1px solid rgba(139, 92, 246, 0.2);
    border-radius: var(--ch-radius-md);
    font-size: 0.875rem;
    font-weight: var(--ch-font-weight-medium);
    color: var(--ch-text-primary);
    transition: all 0.2s ease;
}

.hero-feature:hover {
    background: rgba(139, 92, 246, 0.15);
    border-color: rgba(139, 92, 246, 0.3);
    transform: translateY(-1px);
}

.hero-feature__icon {
    width: 20px;
    height: 20px;
    flex-shrink: 0;
    color: var(--ch-brand-primary);
}

.hero-feature__icon svg {
    width: 100%;
    height: 100%;
    stroke: currentColor;
}



.ch-hero__visual {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.content-preview {
    background: var(--ch-surface-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-lg);
    box-shadow: var(--ch-shadow-lg);
    overflow: hidden;
    width: 100%;
    max-width: 500px;
    transform: perspective(1000px) rotateY(-5deg) rotateX(2deg);
    transition: transform 0.3s ease;
}

.content-preview:hover {
    transform: perspective(1000px) rotateY(-2deg) rotateX(1deg);
}


.content-editor {
    padding: 0;
}

.editor-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 1rem;
    background: var(--ch-surface-secondary);
    border-bottom: 1px solid var(--ch-border-primary);
}

.editor-tabs {
    display: flex;
    gap: 0.5rem;
}

.tab {
    padding: 0.5rem 1rem;
    font-size: 0.875rem;
    color: var(--ch-text-secondary);
    background: transparent;
    border-radius: var(--ch-radius-sm);
    transition: all 0.2s ease;
    cursor: pointer;
}

.tab--active {
    color: var(--ch-brand-primary);
    background: var(--ch-brand-50);
}

.editor-actions {
    display: flex;
    gap: 0.5rem;
}

.ch-btn--small {
    padding: 0.375rem 0.75rem;
    font-size: 0.75rem;
}

.ch-btn--primary {
    background: var(--ch-brand-primary);
    color: white;
    border: none;
    border-radius: var(--ch-radius-sm);
    font-weight: var(--ch-font-weight-medium);
    cursor: pointer;
    transition: all 0.2s ease;
}

.ch-btn--primary:hover {
    background: var(--ch-brand-secondary);
    transform: translateY(-1px);
}

.editor-content {
    padding: 1.5rem;
    min-height: 200px;
}

.content-line {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0.5rem 0;
    border-bottom: 1px solid var(--ch-border-secondary);
}

.content-line:last-child {
    border-bottom: none;
}

.content-text {
    font-size: 0.875rem;
    color: var(--ch-text-primary);
}

.ai-suggestion {
    font-size: 0.75rem;
    padding: 0.25rem 0.5rem;
    background: rgba(139, 92, 246, 0.1);
    color: var(--ch-brand-primary);
    border-radius: var(--ch-radius-sm);
}

.content-line--placeholder {
    opacity: 0.6;
}

.typing-indicator {
    color: var(--ch-text-secondary);
    position: relative;
}

.typing-indicator::after {
    content: '|';
    animation: blink 1s infinite;
}

@keyframes blink {
    0%, 50% { opacity: 1; }
    51%, 100% { opacity: 0; }
}


.seo-panel {
    padding: 1rem;
    background: var(--ch-surface-tertiary);
    border-top: 1px solid var(--ch-border-primary);
}

.seo-panel h4 {
    margin: 0 0 1rem 0;
    font-size: 0.875rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
}

.seo-score {
    font-size: 2rem;
    font-weight: var(--ch-font-weight-bold);
    color: var(--ch-success-600);
    text-align: center;
    margin-bottom: 1rem;
}

.seo-metrics {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;
}

.metric {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0.25rem 0;
}

.metric-label {
    font-size: 0.75rem;
    color: var(--ch-text-secondary);
}

.metric-status {
    font-size: 0.875rem;
    font-weight: var(--ch-font-weight-bold);
}

.status--good {
    color: var(--ch-success-600);
}

.status--warning {
    color: var(--ch-warning-600);
}



@media (max-width: 768px) {
    .ch-hero {
        padding: 3rem 0 4rem 0;
    }

    .ch-hero .container {
        grid-template-columns: 1fr;
        gap: 2rem;
        text-align: center;
    }

    .ch-hero__title {
        font-size: clamp(2rem, 6vw, 2.5rem);
    }

    .ch-hero__subtitle {
        font-size: 1.125rem;
        max-width: none;
    }

    .ch-hero__features {
        justify-content: center;
    }

    .hero-feature {
        padding: 0.5rem 0.75rem;
        font-size: 0.8rem;
    }

    .content-preview {
        transform: none;
        max-width: 400px;
    }

    .content-preview:hover {
        transform: none;
    }
}

@media (max-width: 480px) {
    .ch-hero {
        padding: 2rem 0 3rem 0;
    }

    .ch-hero__features {
        flex-direction: column;
        align-items: center;
    }

    .hero-feature {
        width: 100%;
        max-width: 280px;
        justify-content: center;
    }

    .editor-header {
        flex-direction: column;
        gap: 1rem;
        align-items: stretch;
    }

    .editor-tabs {
        justify-content: center;
    }
}



[data-theme="dark"] .ch-hero::before {
    background: radial-gradient(ellipse at center top, 
        rgba(139, 92, 246, 0.15) 0%, 
        transparent 50%);
}

[data-theme="dark"] .content-preview {
    box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.3), 
                0 10px 10px -5px rgba(0, 0, 0, 0.2);
}

[data-theme="light"] .ch-hero {
    background: linear-gradient(135deg, 
        var(--ch-light-bg-primary) 0%, 
        var(--ch-light-surface-primary) 50%, 
        var(--ch-light-bg-primary) 100%);
}

[data-theme="light"] .ch-hero::before {
    background: radial-gradient(ellipse at center top, 
        rgba(139, 92, 246, 0.08) 0%, 
        transparent 50%);
}



@media (prefers-reduced-motion: reduce) {
    .gradient-text {
        animation: none;
    }

    .typing-indicator::after {
        animation: none;
    }

    .content-preview,
    .content-preview:hover {
        transform: none;
    }

    .hero-feature {
        transition: none;
    }

    .ch-btn--primary {
        transition: none;
    }
}

@media (prefers-contrast: high) {
    .hero-feature {
        border-width: 2px;
    }

    .content-preview {
        border-width: 2px;
    }

    .ch-btn--primary {
        border: 2px solid var(--ch-brand-primary);
    }
} 




.ch-header {
    position: sticky;
    top: 0;
    z-index: var(--ch-z-sticky);
    background-color: var(--ch-surface-primary);
    border-bottom: 1px solid var(--ch-border-primary);
    backdrop-filter: blur(8px);
    -webkit-backdrop-filter: blur(8px);
}

.ch-header__container {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 4rem;
    padding: 0 var(--ch-space-4);
    max-width: 1440px;
    margin: 0 auto;
}


.ch-header__brand {
    display: flex;
    align-items: center;
}

.ch-brand-link {
    display: flex;
    align-items: center;
    gap: var(--ch-space-3);
    text-decoration: none;
    color: inherit;
}

.ch-header__logo svg {
    width: 2rem;
    height: 2rem;
}

.ch-header__brand-text {
    display: flex;
    flex-direction: column;
}

.ch-header__brand-name {
    font-family: var(--ch-font-family-heading);
    font-size: var(--ch-font-size-lg);
    font-weight: var(--ch-font-weight-bold);
    color: var(--ch-text-primary);
    line-height: 1.2;
}

.ch-header__brand-subtitle {
    font-size: var(--ch-font-size-xs);
    color: var(--ch-text-secondary);
    line-height: 1;
}


.ch-header__nav {
    display: none;
}

@media (min-width: 768px) {
    .ch-header__nav {
        display: block;
    }
}

.ch-nav__list {
    display: flex;
    align-items: center;
    gap: var(--ch-space-6);
    list-style: none;
}

.ch-nav__link {
    font-size: var(--ch-font-size-sm);
    font-weight: var(--ch-font-weight-medium);
    color: var(--ch-text-secondary);
    text-decoration: none;
    padding: var(--ch-space-2) 0;
    position: relative;
    transition: color var(--ch-transition-fast);
}

.ch-nav__link:hover,
.ch-nav__link--active {
    color: var(--ch-primary-600);
}

.ch-nav__link--active::after {
    content: '';
    position: absolute;
    bottom: -1px;
    left: 0;
    right: 0;
    height: 2px;
    background-color: var(--ch-primary-600);
    border-radius: var(--ch-radius-full);
}


.ch-header__actions {
    display: flex;
    align-items: center;
    gap: var(--ch-space-2);
}


[data-theme="light"] .ch-nav__link:hover,
[data-theme="light"] .ch-nav__link--active {
    color: var(--ch-primary-800);
}


@media (max-width: 767px) {
    .ch-header__container {
        padding: 0 var(--ch-space-4);
    }

    .ch-header__brand-text {
        display: none;
    }
}


@media print {
    .ch-header {
        display: none;
    }
} 





.footer {
    background: var(--ch-bg-secondary);
    border-top: 1px solid var(--ch-border-primary);
    padding: var(--ch-space-8) 0 var(--ch-space-4);
    margin-top: auto;
}

.footer__content {
    display: grid;
    grid-template-columns: 1fr 3fr;
    gap: var(--ch-space-8);
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 var(--ch-space-4);
}

.footer__brand {
    display: flex;
    gap: var(--ch-space-4);
    align-items: flex-start;
}

.footer__logo svg {
    width: 32px;
    height: 32px;
}

.footer__brand-name {
    font-size: var(--ch-text-lg);
    font-weight: var(--ch-font-weight-bold);
    color: var(--ch-text-primary);
    margin-bottom: var(--ch-space-1);
}

.footer__brand-subtitle {
    font-size: var(--ch-text-sm);
    color: var(--ch-text-secondary);
    margin-bottom: var(--ch-space-3);
}

.footer__description {
    font-size: var(--ch-text-sm);
    color: var(--ch-text-secondary);
    line-height: var(--ch-line-height-relaxed);
    max-width: 300px;
}

.footer__links {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
    gap: var(--ch-space-6);
}

.footer__links h4 {
    font-size: var(--ch-text-base);
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin-bottom: var(--ch-space-3);
}

.footer__link {
    display: block;
    font-size: var(--ch-text-sm);
    color: var(--ch-text-secondary);
    text-decoration: none;
    margin-bottom: var(--ch-space-2);
    transition: color 0.2s ease;
}

.footer__link:hover {
    color: var(--ch-brand-primary);
}

.footer__bottom {
    border-top: 1px solid var(--ch-border-primary);
    padding-top: var(--ch-space-4);
    margin-top: var(--ch-space-6);
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: var(--ch-text-xs);
    color: var(--ch-text-secondary);
}

.footer__status {
    display: flex;
    align-items: center;
    gap: var(--ch-space-2);
}

.status--success {
    width: 8px;
    height: 8px;
    background: var(--ch-success-primary);
    border-radius: 50%;
}



@media (max-width: 768px) {
    .footer__content {
        grid-template-columns: 1fr;
        gap: var(--ch-space-6);
    }

    .footer__brand {
        flex-direction: column;
        gap: var(--ch-space-2);
    }

    .footer__links {
        grid-template-columns: repeat(2, 1fr);
        gap: var(--ch-space-4);
    }

    .footer__bottom {
        flex-direction: column;
        gap: var(--ch-space-2);
        text-align: center;
    }
} 





.ch-modal-overlay {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
    backdrop-filter: blur(4px);
    -webkit-backdrop-filter: blur(4px);
    z-index: var(--ch-z-modal, 1000);
    display: flex;
    align-items: center;
    justify-content: center;
    padding: var(--ch-space-4);
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
}

.ch-modal-overlay.active {
    opacity: 1;
    visibility: visible;
}

.ch-modal {
    background: var(--ch-bg-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-lg);
    box-shadow: var(--ch-shadow-elevated);
    max-width: 600px;
    width: 100%;
    max-height: 90vh;
    overflow: hidden;
    transform: scale(0.9) translateY(-20px);
    transition: transform 0.3s ease;
}

.ch-modal-overlay.active .ch-modal {
    transform: scale(1) translateY(0);
}

.ch-modal-header {
    padding: var(--ch-space-6);
    border-bottom: 1px solid var(--ch-border-secondary);
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.ch-modal-title {
    font-size: var(--ch-text-xl);
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0;
}

.ch-modal-close {
    background: none;
    border: none;
    font-size: var(--ch-text-xl);
    color: var(--ch-text-secondary);
    cursor: pointer;
    padding: var(--ch-space-2);
    border-radius: var(--ch-radius-sm);
    transition: all 0.2s ease;
}

.ch-modal-close:hover {
    background: var(--ch-interactive-hover);
    color: var(--ch-text-primary);
}

.ch-modal-body {
    padding: var(--ch-space-6);
    overflow-y: auto;
    max-height: calc(90vh - 140px);
}

.ch-modal-footer {
    padding: var(--ch-space-6);
    border-top: 1px solid var(--ch-border-secondary);
    display: flex;
    gap: var(--ch-space-3);
    justify-content: flex-end;
}



.ch-modal--small {
    max-width: 400px;
}

.ch-modal--large {
    max-width: 800px;
}

.ch-modal--fullscreen {
    max-width: 95vw;
    max-height: 95vh;
}



@keyframes modal-fade-in {
    from {
        opacity: 0;
        transform: scale(0.9) translateY(-20px);
    }
    to {
        opacity: 1;
        transform: scale(1) translateY(0);
    }
}

@keyframes modal-fade-out {
    from {
        opacity: 1;
        transform: scale(1) translateY(0);
    }
    to {
        opacity: 0;
        transform: scale(0.9) translateY(-20px);
    }
}



@media (max-width: 768px) {
    .ch-modal-overlay {
        padding: var(--ch-space-2);
    }

    .ch-modal {
        max-height: 95vh;
    }

    .ch-modal-header,
    .ch-modal-body,
    .ch-modal-footer {
        padding: var(--ch-space-4);
    }

    .ch-modal-footer {
        flex-direction: column;
    }
} 





.container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 var(--ch-space-4);
    width: 100%;
}

@media (max-width: 1240px) {
    .container {
        padding: 0 var(--ch-space-3);
    }
}

@media (max-width: 768px) {
    .container {
        padding: 0 var(--ch-space-2);
    }
}



.section-title {
    font-size: clamp(1.75rem, 3vw, 2.25rem);
    font-weight: var(--ch-font-weight-bold);
    color: var(--ch-text-primary);
    text-align: center;
    margin-bottom: 3rem;
    line-height: 1.2;
}



.btn {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 0.5rem;
    padding: 0.75rem 1.5rem;
    font-size: 0.875rem;
    font-weight: var(--ch-font-weight-medium);
    line-height: 1;
    border: 1px solid transparent;
    border-radius: var(--ch-radius-md);
    cursor: pointer;
    transition: all 0.2s ease;
    text-decoration: none;
    white-space: nowrap;
}

.btn--primary {
    background: var(--ch-brand-primary);
    color: white;
    border-color: var(--ch-brand-primary);
}

.btn--primary:hover {
    background: var(--ch-brand-secondary);
    border-color: var(--ch-brand-secondary);
    transform: translateY(-1px);
}

.btn--outline {
    background: transparent;
    color: var(--ch-brand-primary);
    border-color: var(--ch-brand-primary);
}

.btn--outline:hover {
    background: var(--ch-brand-primary);
    color: white;
}

.btn--ghost {
    background: transparent;
    color: var(--ch-text-secondary);
    border-color: transparent;
}

.btn--ghost:hover {
    background: var(--ch-surface-secondary);
    color: var(--ch-text-primary);
}

.btn--small {
    padding: 0.5rem 1rem;
    font-size: 0.75rem;
}

.btn--full {
    width: 100%;
}

.btn--icon {
    padding: 0.75rem;
    width: auto;
    min-width: auto;
}



.tools {
    padding: 4rem 0;
    background: var(--ch-surface-primary);
}

.tools-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
    gap: 2rem;
    margin-top: 2rem;
}

.tool-card {
    background: var(--ch-bg-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-lg);
    padding: 1.5rem;
    transition: all 0.3s ease;
    box-shadow: var(--ch-shadow-sm);
}

.tool-card:hover {
    transform: translateY(-4px);
    box-shadow: var(--ch-shadow-lg);
    border-color: var(--ch-brand-primary);
}

.tool-icon {
    width: 48px;
    height: 48px;
    background: var(--ch-brand-50);
    border-radius: var(--ch-radius-md);
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 1rem;
    color: var(--ch-brand-primary);
}

.tool-icon svg {
    width: 24px;
    height: 24px;
}

.tool-content {
    display: flex;
    flex-direction: column;
    gap: 1rem;
}

.tool-title {
    font-size: 1.25rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0;
}

.tool-description {
    color: var(--ch-text-secondary);
    line-height: 1.6;
    margin: 0;
}

.tool-features {
    display: flex;
    flex-wrap: wrap;
    gap: 0.5rem;
}



.feature-badge {
    display: inline-flex;
    align-items: center;
    padding: 0.25rem 0.75rem;
    font-size: 0.75rem;
    font-weight: var(--ch-font-weight-medium);
    background: var(--ch-surface-secondary);
    color: var(--ch-text-secondary);
    border-radius: var(--ch-radius-full);
    border: 1px solid var(--ch-border-secondary);
}



.templates {
    padding: 4rem 0;
    background: var(--ch-bg-primary);
}

.template-categories {
    display: flex;
    justify-content: center;
    gap: 1rem;
    margin-bottom: 3rem;
    flex-wrap: wrap;
}

.category-btn {
    padding: 0.75rem 1.5rem;
    background: transparent;
    color: var(--ch-text-secondary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-full);
    cursor: pointer;
    transition: all 0.2s ease;
    font-size: 0.875rem;
    font-weight: var(--ch-font-weight-medium);
}

.category-btn--active,
.category-btn:hover {
    background: var(--ch-brand-primary);
    color: white;
    border-color: var(--ch-brand-primary);
}

.template-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 1.5rem;
}

.template-card {
    background: var(--ch-surface-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-lg);
    overflow: hidden;
    transition: all 0.3s ease;
}

.template-card:hover {
    transform: translateY(-2px);
    box-shadow: var(--ch-shadow-md);
    border-color: var(--ch-brand-primary);
}

.template-preview {
    padding: 1.5rem;
    background: var(--ch-surface-secondary);
    border-bottom: 1px solid var(--ch-border-primary);
}

.template-info {
    padding: 1.5rem;
}

.template-info h4 {
    font-size: 1.125rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0 0 0.5rem 0;
}

.template-info p {
    color: var(--ch-text-secondary);
    margin: 0 0 1rem 0;
    line-height: 1.5;
}

.preview-header {
    font-size: 0.75rem;
    color: var(--ch-text-secondary);
    margin-bottom: 0.75rem;
    text-transform: uppercase;
    letter-spacing: 0.05em;
}

.preview-content {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;
}

.preview-line {
    height: 8px;
    background: var(--ch-border-secondary);
    border-radius: var(--ch-radius-sm);
}

.preview-line.long {
    width: 100%;
}

.preview-line.medium {
    width: 75%;
}

.preview-line.short {
    width: 50%;
}

.preview-square {
    width: 40px;
    height: 40px;
    background: var(--ch-border-secondary);
    border-radius: var(--ch-radius-sm);
    margin-bottom: 0.5rem;
}



.workspace {
    padding: 4rem 0;
    background: var(--ch-surface-primary);
}

.workspace-layout {
    display: grid;
    grid-template-columns: 280px 1fr 320px;
    gap: 2rem;
    margin-top: 2rem;
}

.workspace-sidebar {
    background: var(--ch-bg-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-lg);
    padding: 1.5rem;
    height: fit-content;
}

.workspace-main {
    background: var(--ch-bg-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-lg);
    display: flex;
    flex-direction: column;
}

.workspace-analytics {
    background: var(--ch-bg-primary);
    border: 1px solid var(--ch-border-primary);
    border-radius: var(--ch-radius-lg);
    padding: 1.5rem;
    height: fit-content;
}



.sidebar-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 1.5rem;
}

.sidebar-header h3 {
    font-size: 1.125rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0;
}

.project-list {
    display: flex;
    flex-direction: column;
    gap: 0.75rem;
}

.project-item {
    display: flex;
    align-items: center;
    gap: 0.75rem;
    padding: 0.75rem;
    border-radius: var(--ch-radius-md);
    cursor: pointer;
    transition: all 0.2s ease;
}

.project-item:hover,
.project-item--active {
    background: var(--ch-surface-secondary);
}

.project-icon {
    width: 32px;
    height: 32px;
    background: var(--ch-surface-secondary);
    border-radius: var(--ch-radius-sm);
    display: flex;
    align-items: center;
    justify-content: center;
    color: var(--ch-text-secondary);
}

.project-info {
    flex: 1;
}

.project-name {
    font-size: 0.875rem;
    font-weight: var(--ch-font-weight-medium);
    color: var(--ch-text-primary);
    margin: 0;
}

.project-status {
    font-size: 0.75rem;
    color: var(--ch-text-secondary);
    margin: 0;
}



.analytics-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 1.5rem;
}

.analytics-header h3 {
    font-size: 1.125rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0;
}

.analytics-period {
    font-size: 0.75rem;
    color: var(--ch-text-secondary);
}

.analytics-metrics {
    display: grid;
    grid-template-columns: 1fr;
    gap: 1rem;
    margin-bottom: 2rem;
}

.metric-card {
    background: var(--ch-surface-secondary);
    border-radius: var(--ch-radius-md);
    padding: 1rem;
    text-align: center;
}

.metric-value {
    font-size: 1.5rem;
    font-weight: var(--ch-font-weight-bold);
    color: var(--ch-text-primary);
    margin-bottom: 0.25rem;
}

.metric-label {
    font-size: 0.75rem;
    color: var(--ch-text-secondary);
    margin-bottom: 0.5rem;
}

.metric-change {
    font-size: 0.75rem;
    font-weight: var(--ch-font-weight-medium);
}

.metric-change.positive {
    color: var(--ch-success-600);
}

.metric-change.negative {
    color: var(--ch-error-600);
}

.content-performance h4 {
    font-size: 1rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0 0 1rem 0;
}

.performance-list {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;
}

.performance-item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0.5rem;
    background: var(--ch-surface-secondary);
    border-radius: var(--ch-radius-sm);
}

.performance-title {
    font-size: 0.875rem;
    color: var(--ch-text-primary);
}

.performance-score {
    font-size: 0.75rem;
    font-weight: var(--ch-font-weight-medium);
    color: var(--ch-success-600);
}



.editor-toolbar {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 1rem 1.5rem;
    background: var(--ch-surface-secondary);
    border-bottom: 1px solid var(--ch-border-primary);
}

.toolbar-left,
.toolbar-right {
    display: flex;
    align-items: center;
    gap: 0.75rem;
}

.word-count {
    font-size: 0.875rem;
    color: var(--ch-text-secondary);
}

.editor-area {
    flex: 1;
    padding: 2rem;
    min-height: 400px;
}

.editor-placeholder {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    height: 100%;
    color: var(--ch-text-secondary);
}

.placeholder-icon {
    width: 64px;
    height: 64px;
    background: var(--ch-surface-secondary);
    border-radius: var(--ch-radius-lg);
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 1.5rem;
    color: var(--ch-text-secondary);
}

.editor-placeholder h3 {
    font-size: 1.25rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0 0 0.5rem 0;
}

.editor-placeholder p {
    margin: 0 0 1.5rem 0;
    max-width: 400px;
}



.footer {
    background: var(--ch-surface-primary);
    border-top: 1px solid var(--ch-border-primary);
    padding: 3rem 0 2rem 0;
    margin-top: 4rem;
}

.footer__content {
    display: grid;
    grid-template-columns: 2fr 1fr 1fr 1fr 1fr;
    gap: 3rem;
    margin-bottom: 2rem;
}

.footer__brand {
    display: flex;
    gap: 1rem;
}

.footer__logo {
    flex-shrink: 0;
}

.footer__brand-name {
    font-size: 1.125rem;
    font-weight: var(--ch-font-weight-bold);
    color: var(--ch-text-primary);
    margin-bottom: 0.25rem;
}

.footer__brand-subtitle {
    font-size: 0.875rem;
    color: var(--ch-brand-primary);
    margin-bottom: 0.75rem;
}

.footer__description {
    font-size: 0.875rem;
    color: var(--ch-text-secondary);
    line-height: 1.6;
    margin: 0;
}

.footer__links h4 {
    font-size: 0.875rem;
    font-weight: var(--ch-font-weight-semibold);
    color: var(--ch-text-primary);
    margin: 0 0 1rem 0;
}

.footer__link {
    display: block;
    font-size: 0.875rem;
    color: var(--ch-text-secondary);
    text-decoration: none;
    margin-bottom: 0.5rem;
    transition: color 0.2s ease;
}

.footer__link:hover {
    color: var(--ch-brand-primary);
}

.footer__bottom {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-top: 2rem;
    border-top: 1px solid var(--ch-border-primary);
    font-size: 0.875rem;
    color: var(--ch-text-secondary);
}

.footer__status {
    display: flex;
    align-items: center;
    gap: 0.5rem;
}

.status--success {
    width: 8px;
    height: 8px;
    background: var(--ch-success-600);
    border-radius: 50%;
}



@media (max-width: 1024px) {
    .workspace-layout {
        grid-template-columns: 1fr;
        gap: 1.5rem;
    }

    .footer__content {
        grid-template-columns: 1fr 1fr;
        gap: 2rem;
    }
}

@media (max-width: 768px) {
    .tools-grid {
        grid-template-columns: 1fr;
    }

    .template-grid {
        grid-template-columns: 1fr;
    }

    .footer__content {
        grid-template-columns: 1fr;
        gap: 1.5rem;
    }

    .footer__bottom {
        flex-direction: column;
        gap: 1rem;
        text-align: center;
    }

    .editor-toolbar {
